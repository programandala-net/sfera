       = 0000EC         pt_scrsz equ  $ec    ; ; long  SCReen SiZe in bytes
       = 0000F0         pt_scinc equ  $f0    ; ; word  SCreen row INCrement
       = 0000F2         pt_xscrs equ  $f2    ; ; word  X SCReen Size in pixels
       = 0000F4         pt_yscrs equ  $f4    ; ; word  Y SCReen Size in pixels

       = 0000F6         pt_xtotl equ  $f6    ; ; word  X total pixels
       = 0000F8         pt_xvref equ  $f8    ; ; word  X visible reference for xtotl
       = 0000FA         pt_ytotl equ  $fa    ; ; word  Y total pixels
       = 0000FC         pt_yvref equ  $fc    ; ; word  Y visible reference for ytotl
       = 0000FE         pt_frate equ  $fe    ; ; word  frame rate

       = 000102         pt_bgstp equ  $102   ; ; word  background stipple
       = 000104         pt_bgclm equ  $104   ; ; long  background colour mask
       = 000108         pt_sfnt1 equ  $108   ; ; long  pointer to Standard FoNT 1
       = 00010C         pt_sfnt2 equ  $10c   ; ; long  pointer to Standard FoNT 2

