                        ; ==============================================================
                        ; Description

                        ; Constants for QDOS system calls and traps.

                        ; ==============================================================
                        ; Acknowledgment

                        ; This file was created with files and information from:

                        ; GWASL assembler (by George Wilt, 2008)
                        ; QL Advanced User Guide (by Adrian Dickens, 1984)
                        ; SMSQ/E sources (by Tonny Tebby et al.)

                        ; ==============================================================
                        ; History

                        ; 2016-01-11: Added to the library.
                        ; 2016-01-14: Reorganized. Titles.

                        ; ==============================================================
                        ; Traps

                        ; ----------------------------------------------
                        ; Manager traps (trap #1)

       = 000000         mt_inf        equ   $00000000
       = 000001         mt_cjob       equ   $00000001
       = 000002         mt_jinf       equ   $00000002
       = 000004         mt_rjob       equ   $00000004
       = 000005         mt_frjob      equ   $00000005
       = 000006         mt_free       equ   $00000006
       = 000007         mt_trapv      equ   $00000007
       = 000008         mt_susjb      equ   $00000008
       = 000009         mt_reljb      equ   $00000009
       = 00000A         mt_activ      equ   $0000000a
       = 00000B         mt_prior      equ   $0000000b
       = 00000C         mt_alloc      equ   $0000000c
       = 00000D         mt_lnkfr      equ   $0000000d
       = 00000E         mt_alres      equ   $0000000e
       = 00000F         mt_reres      equ   $0000000f
       = 000010         mt_dmode      equ   $00000010
       = 000011         mt_ipcom      equ   $00000011
       = 000012         mt_baud       equ   $00000012
       = 000013         mt_rclck      equ   $00000013
       = 000014         mt_sclck      equ   $00000014
       = 000015         mt_aclck      equ   $00000015
       = 000016         mt_albas      equ   $00000016
       = 000017         mt_rebas      equ   $00000017
       = 000018         mt_alchp      equ   $00000018
       = 000019         mt_rechp      equ   $00000019
       = 00001A         mt_lxint      equ   $0000001a
       = 00001B         mt_rxint      equ   $0000001b
       = 00001C         mt_lpoll      equ   $0000001c
       = 00001D         mt_rpoll      equ   $0000001d
       = 00001E         mt_lschd      equ   $0000001e
       = 00001F         mt_rschd      equ   $0000001f
       = 000020         mt_liod       equ   $00000020
       = 000021         mt_riod       equ   $00000021
       = 000022         mt_ldd        equ   $00000022
       = 000023         mt_rdd        equ   $00000023

                        ; ----------------------------------------------
                        ; IO allocation traps (trap #2)

       = 000001         io_open       equ   $00000001
       = 000002         io_close      equ   $00000002
       = 000003         io_formt      equ   $00000003
       = 000004         io_delet      equ   $00000004

