                        ; ==============================================================
                        ; System variables (SMS2)

                        ; ----------------------------------------------
                        ; Machine identification

       = 000000         sys_idnt equ  $0000  ; long  system variables identifier
       = 540000         sysid_ql equ  $d2540000  ; QL (QDOS) system variable identifier
       = 324154         sysid_at equ  'S2AT'     ; SMS Atari system variable identifier
       = 4D5351         sysid_sq equ  'SMSQ'     ; SMSQ identifier

                        ; ----------------------------------------------
                        ; RAM allocation table

       = 000004         sys_chpb equ  $0004  ; long  Common HeaP Base
       = 000008         sys_chpf equ  $0008  ; long  Common HeaP Free space pointer
       = 00000C         sys_fsbb equ  $000c  ; long  Filing system Slave Block area Base
       = 000010         sys_sbab equ  $0010  ; long  'QL SuperBASIC' Area Base
       = 000014         sys_tpab equ  $0014  ; long  Transient Program Area Base
       = 000018         sys_tpaf equ  $0018  ; long  Transient Program Area Free space pointer
       = 00001C         sys_rpab equ  $001c  ; long  Resident Procedure Area Base
       = 000020         sys_ramt equ  $0020  ; long  user RAM Top
       = 000024         sys_mxfr equ  $0024  ; long  Maximum return from free memory call

       = 000028         sys_rtc  equ  $0028  ; long  real time (seconds)
       = 00002C         sys_rtcf equ  $002c  ; word  real time fractional, count down

                        ; ----------------------------------------------
                        ; Odd working variables

       = 00002E         sys_rand equ  $002e  ; word  RANDom number
       = 000030         sys_pict equ  $0030  ; word  Polling Interupt CounT
       = 000032         sys_dtyp equ  $0032  ; byte  Display TYPe (0=normal, 1=TV 625, 2=TV 525)
       = 000033         sys_dfrz equ  $0033  ; byte  Display FRoZen (T or F)
       = 000034         sys_qlmr equ  $0034  ; byte  QL Master chip Register value
       = 000003         sysqm__8 equ    3      ; mode 8 bit
       = 000035         sys_qlir equ  $0035  ; byte  QL Interrupt Register value
       = 000036         sys_rshd equ  $0036  ; byte  true to reschedule
       = 000037         sys_nnnr equ  $0037  ; byte  Network Node NumbeR
                        ; ----------------------------------------------
                        ; Action routine and driver lists

       = 000038         sys_exil equ  $0038  ; long  EXternal Interrupt action List
       = 00003C         sys_poll equ  $003c  ; long  POLled action List
       = 000040         sys_shdl equ  $0040  ; long  ScHeDuler loop action List
       = 000044         sys_iodl equ  $0044  ; long  IO Driver List
       = 000048         sys_fsdl equ  $0048  ; long  Filing System Driver List

       = 00004C         sys_ckyq equ  $004c  ; long  Current Keyboard Queue

       = 000050         sys_ertb equ  $0050  ; long  Exception Redirection Table Base

                        ; ----------------------------------------------
                        ; Resource allocation

                        ; Filing system slave blocks, 8 byte entries, first byte 0 = unavailable
                        ;              1 = vacant

       = 000054         sys_sbrp equ  $0054  ; long  Slave Block Running Pointer
       = 000058         sys_sbtb equ  $0058  ; long  Slave Block Table Base
       = 00005C         sys_sbtt equ  $005c  ; long  Slave Block Table Top

       = 000060         sys_jbtg equ  $0060  ; word  next JoB TaG
       = 000062         sys_jbtp equ  $0062  ; word  highest JoB in table (ToP one)
       = 000064         sys_jbpt equ  $0064  ; long  current JoB PoinTer
       = 000068         sys_jbtb equ  $0068  ; long  JoB Table Base
       = 00006C         sys_jbtt equ  $006c  ; long  JoB Table Top

       = 000070         sys_chtg equ  $0070  ; word  next CHannel TaG
       = 000072         sys_chtp equ  $0072  ; word  highest CHannel in table (ToP one)
       = 000074         sys_chpt equ  $0074  ; long  last checked CHannel PoinTer
       = 000078         sys_chtb equ  $0078  ; long  CHannel Table Base
       = 00007C         sys_chtt equ  $007c  ; long  CHannel Table Top
       = 000080         sys_frbl equ  $0080  ; long  FRee Block List (to be returned to common heap)
       = 000084         sys_tsdd equ  $0084  ; byte  Thor flag
       = 000084         sys_capr equ  $0084  ; long  Capslock routine address (if positive)

                        ; ----------------------------------------------
                        ; Fixed peripheral working area (QL)

       = 000088         sys_perw equ  $0088  ; to $00ac

       = 000088         sys_caps equ  $0088  ; word  CAPS lock
       = 00008A         sys_lchr equ  $008a  ; word  Last CHaRacter
       = 00008C         sys_rdel equ  $008c  ; word  Repeat DELay (20ms units)
       = 000019         sys__rdel equ     25
       = 00008E         sys_rtim equ  $008e  ; word  Repeat TIMe (20ms units)
       = 000002         sys__rtim equ      2
       = 000090         sys_rcnt equ  $0090  ; word  Repeat CouNTer (decremented every 20ms)
       = 000092         sys_swtc equ  $0092  ; word  SWiTch queues Character
       = 000094         sys_hstt equ  $0094  ; byte  additional hardware status
       = 000095         sys_tmsv equ  $0095  ; byte  sys_tmod saved by MDV routines ****
       = 000096         sys_qlbp equ  $0096  ; byte  QL BeePing
       = 000097         sys_brk  equ  $0097  ; byte  set by keyboard break
       = 000098         sys_qls1r equ $0098  ; long  pointer to QL serial 1 receive buffer
       = 00009C         sys_qls2r equ $009c  ; long  pointer to QL serial 2 receive buffer
       = 0000A0         sys_tmod equ  $00a0  ; byte  QL transmit mode
       = 0000A1         sys_ptyp equ  $00a1  ; byte  processor type $00 = 68000/8, $30 = 68030 etc.
       = 000001         sys_immu equ    $01       ; internal MMU
       = 000002         sys_851m equ    $02       ; 68851 MMU
       = 000004         sys_ifpu equ    $04       ; internal FPU
       = 000008         sys_88xf equ    $08       ; 68881 68882 FPU
       = 0000A2         sys_csub equ  $00a2  ; long  subroutine to jump to on capslock
       = 0000A6         sys_stmo equ  $00a6  ; byte  serial xmit timeout
       = 0000A6         sys_dmiu equ  $00a6  ; byte  Atari msb DMA in use, lsb kill SER2 xmit
       = 0000A6         sys_stiu equ  $00a6  ; byte  msb Sector transfer in use
       = 0000A7         sys_mtyp equ  $00a7  ; byte  Machine TYPe / display type
       = 00001E         sys__mtyp equ    $1e       ; machine ID bits
       = 000001         sys_blit  equ    +1       ; Blitter fitted
       = 000001         sys_herm  equ    +1       ; Hermes fitted
       = 000000         sys_mst   equ   $00       ; ordinary ST
       = 000002         sys_mstr  equ   $02       ; Mega ST or ST with RTC
       = 000004         sys_msta  equ   $04       ; Stacy
       = 000006         sys_mste  equ   $06       ; ordinary STE
       = 000008         sys_mmste equ   $08       ; Mega STE
       = 00000A         sys_mgold equ   $0a       ; Gold card
       = 00000C         sys_msgld equ   $0c       ; SuperGOLD card
       = 000010         sys_mfal  equ   $10       ; Falcon
       = 000011         sys_mq40  equ   $11       ; Q40
       = 000018         sys_mtt   equ   $18       ; TT
       = 00001C         sys_mqxl  equ   $1c       ; QXL
       = 00001E         sys_mqpc  equ   $1e       ; QPC
       = 000014         sys_java  equ   $14       ; java
       = 000000         sys_mfut  equ  %00000000      ; Standard QL or Futura display
       = 000020         sys_mmon  equ  %00100000      ; Monochone monitor
       = 000040         sys_mext  equ  %01000000      ; Atari Extended 4 Emulator
       = 000080         sys_mvme  equ  %10000000      ; Atari QVME emulator
       = 0000C0         sys_mqlc  equ  %11000000      ; QL mode LCD
       = 0000C0         sys_mvga  equ  %11000000      ; VGA
       = 0000A0         sys_maur  equ  %10100000      ; Aurora

       = 0000A8         sys_polf equ  $00a8  ; word  polling frequency
       = 000032         sys__polf equ    50     ; ... assumed polling frequency
       = 0000AA         sys_cfst equ  $00aa  ; word  cursor flash status

                        ; ----------------------------------------------
                        ; Filing system defaults

       = 000070         sys_defo equ    $70    ; offset to make defaults<$80
       = 0000AC         sys_prgd equ  $00ac  ; long  pointer to PRoGram Default
       = 0000B0         sys_datd equ  $00b0  ; long  pointer to DATa Default
       = 0000B4         sys_dstd equ  $00b4  ; long  pointer to DeSTination Default

       = 0000B8         sys_thgl equ  $00b8  ; long  pointer to THinG List
       = 0000BC         sys_psf  equ  $00bc  ; long  Primary stack frame pointer (SMSQ)
       = 0000C0         sys_int2i equ $00c0  ; byte  interrupt 2 in service
       = 0000C1         sys_50i  equ  $00c1  ; byte  50 Hz in service
       = 0000C3         sys_plrq equ  $00c3  ; byte  poll requested (-ve for request)
       = 0000C4         sys_clnk equ  $00c4  ; long  pointer to console linkage
       = 0000C8         sys_castt equ $00c8  ; byte  0 cache on, +ve cache temp off, -ve off
       = 0000C9         sys_casup equ $00c9  ; byte  cache suppressd timer
       = 00001A         sys__casup equ    26  ; byte  25 full ticks
       = 0000CA         sys_iopr  equ $00ca  ; word  IO priority
       = 0000CC         sys_cbas  equ $00cc  ; long  current basic (copy of sys_jbpt)

       = 0000D0         sys_fpu   equ $00d0  ; 16 bytes

       = 0000E0         sys_prtc  equ $00e0  ; byte set if real time clock protected
       = 0000E1         sys_pmem  equ $00e1  ; byte memory protection level
       = 0000E2         sys_slug  equ $00e2  ; word slug level
       = 0000E4         sys_klock equ $00e4  ; byte key lock
       = 000000         sys__shk  equ     0        ; suppress HOTKEY
       = 000001         sys__ssf  equ     1        ; suppress screen freeze
       = 000002         sys__ssq  equ     2        ; suppress switch queue
       = 000004         sys__sbk  equ     4        ; suppress BREAK
       = 000006         sys__ssr  equ     6        ; suppress keyboard soft reset
       = 000007         sys__shr  equ     7        ; suppress keyboard hard reset
       = 0000E6         sys_mtick equ $00e6   ; word mini tick counter
       = 0000E8         sys_klnk  equ $00e8   ; long pointer to keyboard linkage

                        ; ----------------------------------------------
                        ; Fixed filing system working area (QL)

       = 0000EE         sys_filw equ  $00ee  ; to $0100

                        ; ----------------------------------------------
                        ; Filing system tables

       = 000100         sys_fsdd equ  $0100  ; 16*long pointers to Filing System Drive Definitions
       = 000140         sys_fsdt equ  $0140    ; Filing System drive Definition table Top
       = 000010         sys_nfsd equ  $10    ; max Number of Filing System Drive definitions
       = 000140         sys_fsch equ  $0140  ; long  linked list of Filing System CHannel blocks

       = 000144         sys_xact equ  $0144  ; byte  set if XLATE active
       = 000146         sys_xtab equ  $0146  ; long  pointer to XLATE table
       = 00014A         sys_erms equ  $014a  ; long  pointer to (QDOS) error message table
       = 00014E         sys_mstab equ $014e  ; long  pointer to (SMSQ) message table
       = 000154         sys_taskm equ $0154  ; 4 long  used by taskmaster - conflicts with
       = 000160         sys_turbo equ $0160  ; long  used by turbo
       = 000164         sys_qsound equ  $0164  ; long  used by qsound

       = 000168         sys_ldmlst equ  $0168  ; long  language dependent module list
       = 00016C         sys_lang equ  $016c  ; word  current language

       = 000170         sys_vers equ  $0170  ; long  version
       = 00017E         sys_xdly equ  $017e  ; byte  suspend delay after executing another job (3.13)
       = 00017F         sys_ouch equ  $017f  ; byte  ouch flag (currently used to activate SGC debug)
       = 000180         sys_top  equ  $0180    ; TOP of system vars - bottom of Supervisor Stack

